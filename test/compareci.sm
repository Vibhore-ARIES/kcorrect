data testsp.K.ci.dat
read <uci 1 gci 2 rci 3 ici 4 zci 5>		
define nlines (dimen(uci))
data testsp.K.maggies.z0.1.dat
lines 0 $nlines
read <u 1 g 2 r 3 i 4 z 5>		
data testsp.K.maggies.dat
read <u0 1 g0 2 r0 3 i0 4 z0 5>		
data testsp.K.dat
lines 0 $nlines
read <zz 1 uci0 2 gci0 3 rci0 4 ici0 5 zci0 6 >

set Kudiff = -2.5*lg((uci/uci0)/(u/u0))
set Kgdiff = -2.5*lg((gci/gci0)/(g/g0))
set Krdiff = -2.5*lg((rci/rci0)/(r/r0))
set Kidiff = -2.5*lg((ici/ici0)/(i/i0))
set Kzdiff = -2.5*lg((zci/zci0)/(z/z0))

define labellinesize 1.2
define boxlinesize 1.2
define linesize 1.1

hvfracsep 1 5 1. 1 1 1.
limits 0. 0.2 -0.25 0.25 
putbox 0 2 0 0
ptype 1 1
points zz Kudiff
ylab \Delta K_{^{0.1}u}

hvfracsep 2 5 1. 1 1 1.
limits 0. 0.2 -0.25 0.25 
putbox 0 2 0 0
ptype 1 1
points zz Kgdiff
ylab \Delta K_{^{0.1}g}

hvfracsep 3 5 1. 1 1 1.
limits 0. 0.2 -0.25 0.25 
putbox 0 2 0 0
ptype 1 1
points zz Krdiff
ylab \Delta K_{^{0.1}r}

hvfracsep 4 5 1. 1 1 1.
limits 0. 0.2 -0.25 0.25 
putbox 0 2 0 0
ptype 1 1
points zz Kidiff
ylab \Delta K_{^{0.1}i}

hvfracsep 5 5 1. 1 1 1.
limits 0. 0.2 -0.25 0.25 
putbox 1 2 0 0
ptype 1 1
points zz Kzdiff
ylab \Delta K_{^{0.1}z}

xlab Redshift z

startline 5600 30500
makeline Comparison to power-law interpolation between bands 
